#!/bin/sh
#chmod u+x run.sh 
run()
{
	name=$1
   echo " -- running ${name} -- "
	./bin/scip -s ./settings/default.set -f ../data/${name}.logreg > ./test/${name}.log
   grep objval ./test/${name}.log
}

rm -f ./test/*.log

run pima-indians-diabetes
echo "opt = 739.45"

run mammo
echo "opt = 611.94"

run seismic-bumps
echo "opt = 1097.11"

run parkin
echo "opt = 113.50"

run statH
echo "opt = 195.62"

