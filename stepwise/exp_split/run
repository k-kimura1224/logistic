#!/bin/sh
#chmod u+x run.sh 
run()
{
   name=$1
   rm -rf $name
   mkdir $name

   for i in `seq 0 9`
   do
      mkdir ${name}/${i}
      for j in `seq 0 4`
      do
	      export OMP_NUMTHREADS=1
	      ../bin/scip -f ../../Split_data/output/${name}/${i}/${j}_sample_${name}.logreg  > ./${j}.log
      done
      mv *.sol *.log ${name}/${i}
   done
}

rm -f *.sol
rm -f *.log

run parkin
run seismic-bumps
run statG
run statH
